{
  "name": "aam-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/aam-web/src",
  "tags": ["type:app", "scope:web", "lang:typescript"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/aam-web"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "aam-web:build",
        "port": 3000
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "aam-web:build",
        "port": 4300
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "options": {
        "reportsDirectory": "../../coverage/apps/aam-web"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
